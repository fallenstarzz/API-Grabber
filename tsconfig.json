{  "compilerOptions": {    "target": "ES2020",    "module": "commonjs",    "lib": ["ES2020", "DOM", "DOM.Iterable"],    "moduleResolution": "node",    "esModuleInterop": true,    "allowSyntheticDefaultImports": true,    "resolveJsonModule": true,    "outDir": "./dist",    "rootDir": "./src",    "removeComments": true,    "sourceMap": true,    "strict": true,    "noImplicitAny": true,    "strictNullChecks": true,    "noUnusedLocals": false,    "noUnusedParameters": false,    "skipLibCheck": true,    "forceConsistentCasingInFileNames": true  },  "include": ["src/**/*"],  "exclude": ["node_modules", "dist", "logs", "recordings"]} 
