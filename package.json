{
  "name": "euclid-swap-bot",
  "version": "2.0.0",
  "description": "Advanced Swap Recording Bot with Input Detection",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "npm run build:force && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
